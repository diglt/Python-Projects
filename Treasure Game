print("Welcome to Treasure Island.")
print("Your mission is to find the treasure.") 

FirstChoice = str(input("Left or right? "))

if FirstChoice == "Right" or FirstChoice != "Left":
    print("Fall into a hole. Game Over.")
else:
    Swim_Or_Wait = str(input("Swim or wait "))
    if Swim_Or_Wait == "Swim" or Swim_Or_Wait != "wait":
        print("Attacked by trout. Game Over.")
    else:
        Choose_Door = str(input("Which Door? "))
        if Choose_Door == "Blue":
            print("Eaten by beasts. Game Over")
        elif Choose_Door == "Yellow":
            print("You win!")
        elif Choose_Door == "Red":
            print("Burned by fire. Game Over.")
        elif Choose_Door != "Red" or Choose_Door != "Blue" or Choose_Door != "Yellow":
            print("Game Over.")

